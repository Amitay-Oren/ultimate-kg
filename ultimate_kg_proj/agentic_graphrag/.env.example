# Agentic GraphRAG A2A System Configuration

# A2A Server Configuration
A2A_SERVER_HOST=localhost
A2A_SERVER_PORT=8080
A2A_NETWORK_MODE=development
A2A_PROTOCOL_VERSION=1.0
A2A_MAX_CONCURRENT_REQUESTS=100
A2A_REQUEST_TIMEOUT=30

# Security Configuration
A2A_TLS_ENABLED=false
A2A_TLS_CERT_FILE=
A2A_TLS_KEY_FILE=
A2A_AGENT_API_KEY_SECRET=your-secure-api-key
A2A_NETWORK_SECRET=your-network-secret

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id
VERTEX_AI_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Google ADK Configuration
ADK_MODEL=gemini-2.0-flash
ADK_LOG_LEVEL=INFO

# MCP Server Configuration
MCP_SERVER_URL=http://127.0.0.1:8000/sse
MCP_SERVER_TIMEOUT=30

# Cognee GraphRAG Configuration
LLM_API_KEY=your-openai-or-other-llm-api-key
VECTOR_DB_PROVIDER=lancedb
GRAPH_DATABASE_PROVIDER=neo4j
DB_PROVIDER=sqlite

# Neo4j Configuration
GRAPH_DATABASE_URL=bolt://localhost:7687
GRAPH_DATABASE_USERNAME=neo4j
GRAPH_DATABASE_PASSWORD=password123

# Embedding Configuration
EMBEDDING_PROVIDER=fastembed
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
EMBEDDING_DIMENSIONS=384
EMBEDDING_MAX_TOKENS=256

# Notification Configuration
NOTIFICATION_THRESHOLD=0.7
NOTIFICATION_CHANNELS=console,file
WEBHOOK_URL=

# Monitoring Configuration
A2A_ENABLE_METRICS=true
A2A_METRICS_PORT=9090

# Environment
ENV=development